{
  "facilities":{
    "httpServer":{
      "listenPort": {{ApiHttpPort}}
    }
  },
  "database":{
    "core":{
      "connectionString": "user={{CoreDbApiUser}} dbname={{CoreDb}} host={{CoreDbHost}} password={{CoreDbApiPassword}}"
    }
  },
  "trigger":{
    "lifecycleUri": "http://{{TriggerHost}}:{{TriggerHttpPort}}/job-lifecycle",
    "healthCheckUri": "http://{{TriggerHost}}:{{TriggerHttpPort}}/health-check"
  }
}